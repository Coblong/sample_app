<li>
  <span class="content"><%= micropost.content %></span>
  <% if micropost.robot_config != nil %>
    <div class="versions">
      <%= render partial: 'robot_configs/robot_config_button', locals: {robot_config: micropost.robot_config} %>
    </div>
  <% end %>
  <span class="timestamp">
    Posted <%= time_ago_in_words(micropost.created_at) %> ago.
  </span>
  <%= render 'shared/delete_post', object: micropost %>
</li>